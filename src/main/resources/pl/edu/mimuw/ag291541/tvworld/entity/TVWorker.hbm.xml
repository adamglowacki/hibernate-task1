<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pl.edu.mimuw.ag291541.tvworld.entity">
	<class name="TVWorker" schema="public" table="tvworker">
		<composite-id>
			<key-many-to-one name="identity" column="tvworker_identity"></key-many-to-one>
			<key-many-to-one name="employer" column="tvworker_employer"></key-many-to-one>
		</composite-id>
	</class>
</hibernate-mapping>